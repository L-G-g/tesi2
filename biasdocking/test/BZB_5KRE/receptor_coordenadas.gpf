npts 40 40 40                        # num.grid points in xyz
gridfld 5KRE_A.maps.fld              # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A C OA N                # ligand atom types
receptor 5KRE_A.pdbqt                # macromolecule
gridcenter 21.473 43.269 27.028      # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 5KRE_A.A.map                     # atom-specific affinity map
map 5KRE_A.C.map                     # atom-specific affinity map
map 5KRE_A.OA.map                    # atom-specific affinity map
map 5KRE_A.N.map                     # atom-specific affinity map
elecmap 5KRE_A.e.map                 # electrostatic potential map
dsolvmap 5KRE_A.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
